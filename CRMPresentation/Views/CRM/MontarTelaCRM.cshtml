@using Presentation.Extensions
@using CrossCutting;
@using EntitiesServices.Work_Classes;
@model EntitiesServices.Model.CRM
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="modalContainer">
        <div id="modal" class="modal" tabindex="-1" role="dialog">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        @*titulo gerado dinamicamente*@
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        @*Gerado dinamicamente*@
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="containerModalDesc">
        <div class="modal fade" id="modalDesc" tabindex="-1" role="dialog" aria-labelledby="modalDesc" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="modalDesc">Descrição do Processo</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div id="modalDescBody" class="modal-body">

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="page-title">
        <h3 class="title">Processos - CRM</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaDashboardCRMNovo", "CRM")">Dashboard de Processos</a>
            </li>
            <li class="active">
                <strong>Processos</strong>
            </li>
        </ol>
        <br />
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Filtros</h5>
                </div>

                @using (Html.BeginForm("FiltrarCRM", "CRM", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
                {
                    <div class="ibox-content">
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Funil
                                @Html.DropDownListFor(m => m.FUNI_CD_ID, (SelectList)ViewBag.Funis, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Data Início
                                @Html.TextBoxFor(m => m.CRM1_DT_CRIACAO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id="data1" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Data Final
                                @Html.TextBoxFor(m => m.CRM1_DT_CANCELAMENTO, "{0:dd/MM/yyyy}", new { @class = "form-control date-picker", autofocus = true, @id="data2" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Origem
                                @Html.DropDownListFor(m => m.ORIG_CD_ID, (SelectList)ViewBag.Origem, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Situação
                                @Html.DropDownListFor(m => m.CRM1_IN_ATIVO, (SelectList)ViewBag.Adic, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Favorito
                                @Html.DropDownListFor(m => m.CRM1_IN_ESTRELA, (SelectList)ViewBag.Favorito, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                        </div>
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Temperatura
                                @Html.DropDownListFor(m => m.CRM1_NR_TEMPERATURA, (SelectList)ViewBag.Temp, "Selecionar", new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Nome do Processo
                                @Html.TextBoxFor(m => m.CRM1_NM_NOME, new { @class = "form-control", autofocus = true })
                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                Processo (Número, Nome ou Precatório)
                                @Html.TextBoxFor(m => m.CRM1_DS_DESCRICAO, new { @class = "form-control", autofocus = true })
                            </div>
                        </div>
                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                            <div class="col-lg-12 col-md-12 col-xs-12">                                     
                                @Html.ButtonActionSubmit(text: "Filtrar")
                                @Html.ButtonLinkIndex(text: "Incluir Novo Processo", controller: "CRM", action: "IncluirProcessoCRM", buttonClass: "primary")
                                @Html.ButtonLinkIndex(text: "Retirar Filtro", controller: "CRM", action: "RetirarFiltroCRM")
                                @Html.ButtonLinkIndex(text: "Visualização Kanban", controller: "CRM", action: "MontarTelaCRMKanbaChama")
                                @Html.ButtonLinkIndex(text: "Ações do Usuário", controller: "CRM", action: "VerAcoesUsuarioCRMPrevia1")
                                @Html.ButtonLinkIndex(text: "Propostas", action: "VerPedidosUsuarioCRMPrevia", controller: "CRM")
                                @Html.ButtonLinkIndex(text: "Histórico", controller: "CRM", action: "MontarTelaHistoricoGeral")
                                @Html.ButtonLinkIndex(text: "Gerar Listagem", controller: "CRM", action: "GerarRelatorioListaCRM")
                                @Html.ButtonLinkIndex(text: "Mensagens Enviadas", action: "VerMensagensEnviadas", controller: "CRM", buttonClass: "success")
                                @Html.ButtonLinkIndex(text: "Voltar", action: "MontarTelaDashboardCRMNovo", controller: "CRM")                
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12"  id="Lista">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Processos</h5>
                </div>
                <div class="ibox-content">
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="row">
                        @if (ViewBag.Listas != null && (ViewBag.Listas as List<EntitiesServices.Model.CRM>).Count > 0)
                        {
                            <table class="table table-striped table-bordered table-hover dataTables-example">
                                <thead style="background-color:lightsteelblue">
                                    <tr>
                                        <th class="td-one-action"></th>
                                        <th style="width: auto">Funil</th>
                                        <th style="width: auto">Etapa</th>
                                        <th style="width: auto">Precatório</th>
                                        <th style="width: auto">Título</th>
                                        <th style="width: auto">Próxima Ação</th>
                                        <th style="width: auto">Data Prevista</th>
                                        <th style="width: auto">Origem</th>
                                        <th style="width: auto">Situação</th>
                                        <th style="width: auto">Encerrado</th>
                                        <th class="td-one-action">Ações</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var cond in (ViewBag.Listas as List<EntitiesServices.Model.CRM>).OrderByDescending(p => p.CRM1_DT_CRIACAO))
                                    {
                                        if (cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).ToList().Count == 0)
                                        {
                                            if (cond.CRM1_IN_ATIVO != 2)
                                            {
                                                if (cond.CRM1_CD_ID != (Int32)Session["LinhaAlterada"])
                                                {
                                                    <tr>                                            
                                                        <td class="td-one-action">
                                                            @if (cond.CRM1_IN_ATIVO != 3)
                                                            {
                                                                if (cond.CRM1_IN_ESTRELA == 1)
                                                                {
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-star" onclick="ShowModalEstrelaNao('/CRM/EstrelaNao/@cond.CRM1_CD_ID')" title="Arquivar"></span>
                                                                }
                                                                else if (cond.CRM1_IN_ESTRELA == 0)
                                                                {   
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-star-o" onclick="ShowModalEstrelaSim('/CRM/EstrelaSim/@cond.CRM1_CD_ID')" title="Reativar"></span>
                                                                }
                                                            }
                                                            else
                                                            {
                                                                if (cond.CRM1_IN_ESTRELA == 1)
                                                                {
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-star" title="Arquivar"></span>
                                                                }
                                                                else if (cond.CRM1_IN_ESTRELA == 0)
                                                                {   
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-star-o" title="Reativar"></span>
                                                                }
                                                            }
                                                        </td>

                                                        <td>@cond.FUNIL.FUNI_NM_NOME</td>
                                                        <td>@cond.FUNIL.FUNIL_ETAPA.Where(p => p.FUET_CD_ID == cond.CRM1_IN_STATUS).First().FUET_NM_NOME</td>
                                                        <td><a href="@Url.Action("EditarClienteForm", "CRM", new { id = cond.CLIE_CD_ID })" />@cond.PRECATORIO.PREC_NM_PRECATORIO</td>

                                                        <td>@cond.CRM1_NM_CAMPANHA</td>

                                                        @if (cond.CRM1_IN_ATIVO == 3 || cond.CRM1_IN_ATIVO == 4 || cond.CRM1_IN_ATIVO == 8)
                                                        {
                                                            <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 2)
                                                        {
                                                            <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                        }                                             
                                                        else
                                                        {
                                                            if (cond.CRM1_DT_ENCERRAMENTO == null)
                                                            {
                                                                <td><a href="@Url.Action("AcompanhamentoProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                            }
                                                            else
                                                            {
                                                                <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                            }
                                                        }


                                                        @if (cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).ToList().Count > 0)
                                                        {
                                                            <td style="width: auto">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DS_DESCRICAO</td>
                                                            if (cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.Date >= DateTime.Today.Date)
                                                            {
                                                                <td style="width: auto; color: green; font-weight:bold">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.ToShortDateString()</td>
                                                            }
                                                            else
                                                            {
                                                                <td style="width: auto; color: red; font-weight:bold">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.ToShortDateString()</td>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <td>-</td>
                                                            <td>-</td>
                                                        }

                                                        @if (cond.CRM_ORIGEM != null)
	                                                    {
                                                            <td style="width: auto">@cond.CRM_ORIGEM.CROR_NM_NOME</td>
                                                        }
                                                        else
                                                        {
                                                            <td style="width: auto">-</td>
                                                        }

                                                        @if (cond.CRM1_IN_ATIVO == 1)
	                                                    {
                                                            <td><span class="badge badge-success">Ativo</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 2)
                                                        {
                                                            <td><span class="badge badge-warning">Arquivado</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 3)
                                                        {
                                                            <td><span class="badge badge-warning">Cancelado</span></td>
                                                        }   
                                                        else if (cond.CRM1_IN_ATIVO == 4)
                                                        {
                                                            <td><span class="badge badge-danger">Falhado</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 5)
                                                        {
                                                            <td><span class="badge badge-primary">Sucesso</span></td>
                                                        }

                                                        @if (cond.CRM1_IN_ENCERRADO == 1)
	                                                    {
                                                            <td><span class="badge badge-success">Sim</span></td>
                                                        }
                                                        else
                                                        {
                                                            <td><span class="badge badge-warning">Não</span></td>
                                                        }

                                                        <td class="td-one-action">
                                                            @if (cond.CRM1_IN_ATIVO == 3 || cond.CRM1_IN_ATIVO == 4 || cond.CRM1_IN_ATIVO == 8)
                                                            {
                                                                @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                            }
                                                            else if (cond.CRM1_IN_ATIVO == 2)
                                                            {
                                                                @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                            }                                             
                                                            else
                                                            {
                                                                if (cond.CRM1_DT_ENCERRAMENTO == null)
                                                                {
                                                                    @Html.TableLinkDetails(cond.CRM1_CD_ID, "AcompanhamentoProcessoCRM", "fa-cubes", "Acompanhar")
                                                                    if (((EntitiesServices.Model.PERFIL)Session["PerfilUsuario"]).PERF_IN_CANCELA_CRM == 1)
                                                                    {
                                                                        <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/CRM/ExcluirProcesso/@cond.CRM1_CD_ID')" title="Arquivar"></span>
                                                                        @Html.TableLinkDetails(cond.CRM1_CD_ID, "CancelarProcessoCRM", "fa-times", "Cancelar")
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                                }

                                                            }
                                                        </td>
                                                    </tr>
                                                }
                                                else
                                                {
                                                    <tr style="background-color: lavender" >
                                                        <td class="td-one-action">
                                                            @if (cond.CRM1_IN_ATIVO != 3)
                                                            {
                                                                if (cond.CRM1_IN_ESTRELA == 1)
                                                                {
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-star" onclick="ShowModalEstrelaNao('/CRM/EstrelaNao/@cond.CRM1_CD_ID')" title="Arquivar"></span>
                                                                }
                                                                else if (cond.CRM1_IN_ESTRELA == 0)
                                                                {   
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-star-o" onclick="ShowModalEstrelaSim('/CRM/EstrelaSim/@cond.CRM1_CD_ID')" title="Reativar"></span>
                                                                }
                                                            }
                                                            else
                                                            {
                                                                if (cond.CRM1_IN_ESTRELA == 1)
                                                                {
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-star" title="Arquivar"></span>
                                                                }
                                                                else if (cond.CRM1_IN_ESTRELA == 0)
                                                                {   
                                                                    <span style="color: #337ab7; cursor:pointer;" alt="Reativar" class="tbl-link fa-lg fa fa-star-o" title="Reativar"></span>
                                                                }
                                                            }
                                                        </td>

                                                        <td>@cond.FUNIL.FUNI_NM_NOME</td>
                                                        <td>@cond.FUNIL.FUNIL_ETAPA.Where(p => p.FUET_CD_ID == cond.CRM1_IN_STATUS).First().FUET_NM_NOME</td>
                                                        <td><a href="@Url.Action("EditarClienteForm", "CRM", new { id = cond.CLIE_CD_ID })" />@cond.PRECATORIO.PREC_NM_PRECATORIO</td>

                                                        <td>@cond.CRM1_NM_CAMPANHA</td>

                                                        @if (cond.CRM1_IN_ATIVO == 3 || cond.CRM1_IN_ATIVO == 4 || cond.CRM1_IN_ATIVO == 8)
                                                        {
                                                            <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 2)
                                                        {
                                                            <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                        }                                             
                                                        else
                                                        {
                                                            if (cond.CRM1_DT_ENCERRAMENTO == null)
                                                            {
                                                                <td><a href="@Url.Action("AcompanhamentoProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                            }
                                                            else
                                                            {
                                                                <td><a href="@Url.Action("VisualizarProcessoCRM", "CRM", new { id = cond.CRM1_CD_ID })" />@cond.CRM1_NM_NOME</td>
                                                            }
                                                        }


                                                        @if (cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).ToList().Count > 0)
                                                        {
                                                            <td style="width: auto">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DS_DESCRICAO</td>
                                                            if (cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.Date >= DateTime.Today.Date)
                                                            {
                                                                <td style="width: auto; color: green; font-weight:bold">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.ToShortDateString()</td>
                                                            }
                                                            else
                                                            {
                                                                <td style="width: auto; color: red; font-weight:bold">@cond.CRM_ACAO.Where(p => p.CRAC_IN_STATUS == 1).OrderByDescending(m => m.CRAC_DT_PREVISTA).FirstOrDefault().CRAC_DT_PREVISTA.Value.ToShortDateString()</td>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <td>-</td>
                                                            <td>-</td>
                                                        }

                                                        @if (cond.CRM_ORIGEM != null)
	                                                    {
                                                            <td style="width: auto">@cond.CRM_ORIGEM.CROR_NM_NOME</td>
                                                        }
                                                        else
                                                        {
                                                            <td style="width: auto">-</td>
                                                        }

                                                        @if (cond.CRM1_IN_ATIVO == 1)
	                                                    {
                                                            <td><span class="badge badge-success">Ativo</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 2)
                                                        {
                                                            <td><span class="badge badge-warning">Arquivado</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 3)
                                                        {
                                                            <td><span class="badge badge-warning">Cancelado</span></td>
                                                        }   
                                                        else if (cond.CRM1_IN_ATIVO == 4)
                                                        {
                                                            <td><span class="badge badge-danger">Falhado</span></td>
                                                        }
                                                        else if (cond.CRM1_IN_ATIVO == 5)
                                                        {
                                                            <td><span class="badge badge-primary">Sucesso</span></td>
                                                        }

                                                        @if (cond.CRM1_IN_ENCERRADO == 1)
	                                                    {
                                                            <td><span class="badge badge-success">Sim</span></td>
                                                        }
                                                        else
                                                        {
                                                            <td><span class="badge badge-warning">Não</span></td>
                                                        }

                                                        <td class="td-one-action">
                                                            @if (cond.CRM1_IN_ATIVO == 3 || cond.CRM1_IN_ATIVO == 4 || cond.CRM1_IN_ATIVO == 8)
                                                            {
                                                                @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                            }
                                                            else if (cond.CRM1_IN_ATIVO == 2)
                                                            {
                                                                @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                            }                                             
                                                            else
                                                            {
                                                                if (cond.CRM1_DT_ENCERRAMENTO == null)
                                                                {
                                                                    @Html.TableLinkDetails(cond.CRM1_CD_ID, "AcompanhamentoProcessoCRM", "fa-cubes", "Acompanhar")
                                                                    if (((EntitiesServices.Model.PERFIL)Session["PerfilUsuario"]).PERF_IN_CANCELA_CRM == 1)
                                                                    {
                                                                        <span style="color: #337ab7; cursor:pointer;" alt="Desativar" class="tbl-link fa-lg fa fa-trash-o" onclick="ShowModalExcluir('/CRM/ExcluirProcesso/@cond.CRM1_CD_ID')" title="Arquivar"></span>
                                                                        @Html.TableLinkDetails(cond.CRM1_CD_ID, "CancelarProcessoCRM", "fa-times", "Cancelar")
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    @Html.TableLinkDetails(cond.CRM1_CD_ID, "VisualizarProcessoCRM", "fa-search", "Consultar")
                                                                }

                                                            }
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            else
                                            {
                                                <tr style="background-color: beige">
                                                        <td class="td-one-action">
                                                            @if (cond.CRM1_IN_ATIVO != 3)
                                                            {
